<robot name="betabot" xmlns:xacro="http://www.ros.org/wiki/xacro">

<!-- CALL HEADER FILES FOR MACROS AND GAZEBO PLUGINS -->
  <xacro:include filename="$(find betabot_description)/urdf/betabot.gazebo" />
  <xacro:include filename="$(find betabot_description)/urdf/materials.xacro" />
  <xacro:include filename="$(find betabot_description)/urdf/macros.xacro" />

<!-- ***** BETABOT BASE PART***** -->                                                                              
  <link name="base_footprint"/>

  <joint name="base_joint" type="fixed">
    <parent link="base_footprint"/>
    <child link="base_link"/>
    <origin xyz="0.0 0.0 0.010" rpy="0 0 0"/>
  </joint>

  <link name="base_link">
    <inertial>
      <origin xyz="2.8456E-05 0.02785 0.00053742" rpy="0 0 0" />
      <mass value="5.67773" />
      <inertia ixx="0.019995" ixy="0.0" ixz="0.0" iyy="0.019995" iyz="0.0" izz="0.03675" />
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry> 
        <mesh filename="package://betabot_description/meshes/betabot_base.dae" />
      </geometry>
      <material name="">
        <color rgba="0.79608 0.82353 0.93725 1" />
      </material>
    </visual>
    <!-- COMMENT THE ORGINAL COLLISION MODEL,NEEDS MORE PROCESSING POWER -->
    <!-- <collision>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry> 
        <mesh filename="package://betabot_description/meshes/betabot_base.dae" /> 
      </geometry>
    </collision> -->
  
    <!-- SIMPLIFIED COLLISOION MESH FOR FOR SMOOTH PROCESSING -->
    <collision>
      <geometry>
        <cylinder length="0.004" radius="0.15"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0  0 0.0825"/>
    </collision> 
  </link>

<!-- ***** BETABOT LEFT WHEEL PART ***** -->   
  <link name="left_wheel_link">
    <inertial>
      <origin xyz="1.7347E-17 -7.3726E-17 -0.015" rpy="0 0 0" />
      <mass value="0.5" />
      <inertia ixx="0.001" ixy="0.0" ixz="0.0" iyy="0.001" iyz="0.0" izz="0.001" />
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://betabot_description/meshes/betabot_wheel.dae" />
      </geometry>
      <material name="">
        <color rgba="0.79216 0.81961 0.93333 1" />
      </material>
    </visual>
    <!-- <collision>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://betabot_description/meshes/betabot_wheel.dae" />
      </geometry>
    </collision> -->
    <collision>
      <geometry>
        <cylinder length="0.025" radius="0.0325"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 -0.02"/>
    </collision> 
  </link>

  <!-- JOINT -->
  <joint name="left_wheel_joint" type="continuous">
    <origin xyz="-0.095 0 -0.0125" rpy="-1.5708 1.3735 -1.5708" />
    <parent link="base_link" />
    <child link="left_wheel_link" />
    <axis xyz="0 0 1" />
  </joint>

<!-- ***** BETABOT RIGHT WHEEL PART ***** -->   
  <!-- LINK -->
  <link name="right_wheel_link">
    <inertial>
      <origin xyz="-1.7347E-17 -7.3726E-17 -0.015" rpy="0 0 0" />
      <mass value="0.5" />
      <inertia ixx="0.001" ixy="0.0" ixz="0.0" iyy="0.001" iyz="0.0" izz="0.001" />
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://betabot_description/meshes/betabot_wheel.dae" />
      </geometry>
      <material name="">
        <color rgba="0.79216 0.81961 0.93333 1" />
      </material>
    </visual>
    <!-- <collision>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://betabot_description/meshes/betabot_wheel.dae" />
      </geometry>
    </collision> -->
    <collision>
      <geometry>
        <cylinder length="0.025" radius="0.0325"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0  0 -0.02"/>
    </collision>    
  </link>

  <!-- JOINT -->
  <joint name="right_wheel_joint" type="continuous">
    <origin xyz="0.095 0 -0.0125" rpy="-1.5708 1.5228 1.5708" />
    <parent link="base_link" />
    <child link="right_wheel_link" />
    <axis xyz="0 0 -1" />
  </joint>

<!-- ***** BETABOT FRONT CASTER WHEEL ***** --> 
   <!-- LINK -->
  <link name="front_caster_link">
    <inertial>
      <origin xyz="0 -0.011704 0.02" rpy="0 0 0" />
      <mass value="0.2" />
      <inertia ixx="0.001" ixy="0.0" ixz="0.0" iyy="0.001" iyz="0.0"  izz="0.001" />
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://betabot_description/meshes/betabot_caster.dae" />
      </geometry>
      <material name="">
        <color rgba="0.65098 0.61961 0.58824 1" />
      </material>
    </visual>
    <!-- <collision>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://betabot_description/meshes/betabot_caster.dae" />
      </geometry>
    </collision> -->
    <collision>
      <geometry>
         <sphere radius="0.020"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0"/>
    </collision>      
  </link>

  <!-- JOINT -->
  <joint name="front_caster_joint" type="fixed">
    <origin xyz="0 -0.105 -0.02231" rpy="1.5708 6.123E-17 1.5708" />
    <parent link="base_link" />
    <child link="front_caster_link" />
    <axis xyz="0 0 0" />
  </joint>

<!-- ***** BETABOT BACK CASTER WHEEL ***** --> 
  <!-- LINK -->
  <link name="back_caster_link">
    <inertial>
      <origin xyz="0 0.011704 0.02" rpy="0 0 0" />
      <mass value="0.2" />
      <inertia ixx="0.001" ixy="0.0" ixz="0.0" iyy="0.001" iyz="0.0" izz="0.001" />
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://betabot_description/meshes/betabot_caster.dae" />
      </geometry>
      <material
        name="">
        <color rgba="0.65098 0.61961 0.58824 1" />
      </material>
    </visual>
    <!-- <collision>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://betabot_description/meshes/betabot_caster.dae" />
      </geometry>
    </collision> -->
    <collision>
      <geometry>
         <sphere radius="0.020"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 -0.002 0"/>
    </collision>     
  </link>
  <!-- JOINT -->
  <joint name="back_caster_joint" type="fixed">
    <origin xyz="0 0.105 -0.02231" rpy="1.5708 6.123E-17 1.5708" />
    <parent link="base_link" />
    <child link="back_caster_link" />
    <axis xyz="0 0 0" />
  </joint>

<!-- ***** RASPBERRY PI 8MB CAMERA SIMULATED PART ***** -->
  <!-- LINK -->
  <link name="camera_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 1.570 0" />
      <geometry>
        <mesh filename="package://betabot_description/meshes/raspicam.dae" />
      </geometry>
        <material name="">
        <color rgba="0.65098 0.61961 0.58824 1" />
      </material>
    </visual>
    <inertial>
      <mass value="0.01" />
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <box_inertia m="0.01" x="0.05" y="0.05" z="0.05" />
      <inertia ixx="1e-6" ixy="0" ixz="0" iyy="1e-6" iyz="0" izz="1e-6" />
    </inertial>
  </link>
  <!-- JOINT -->
  <joint name="camera_joint" type="fixed">
    <axis xyz="0 0 0" />
    <origin xyz="0 -0.148 0.040" rpy="0 0 -1.570"/>
    <parent link="base_link"/>
    <child link="camera_link"/>
  </joint>

<!-- ***** RPLiDAR A1 RANGE FINDER SENSOR SIMULATED PART ***** -->
  <!-- LINK -->
  <link name="rplidar_link">
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.1 0.1 0.1"/>
      </geometry>
    </collision>
    <visual>
      <origin xyz="0 0 -0.032" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://betabot_description/meshes/rplidar.dae"/>
      </geometry>
    </visual>
    <inertial>
      <mass value="1e-5" />
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <inertia ixx="1e-6" ixy="0" ixz="0" iyy="1e-6" iyz="0" izz="1e-6" />
    </inertial>
  </link>
  <!-- JOINT -->
  <joint name="rplidar_joint" type="fixed">
    <axis xyz="0 1 0" />
    <origin xyz="0 0 .1" rpy="0 0 0"/>
    <parent link="base_link"/>
    <child link="rplidar_link"/>
  </joint>
     
<!-- ***** IMU SENSOR PART ***** -->
  
  <link name="imu_sensor_link">
      <collision>
          <origin xyz="0 0 0" rpy="0 0 0"/>
          <geometry>
              <box size="0.02 0.02 0.02"/>
          </geometry>
      </collision>
      <visual>
          <origin xyz="0 0 0" rpy="0 0 0"/>
          <geometry>
              <box size="0.02 0.02 0.02"/>
          </geometry>
      </visual>
      <inertial>
          <mass value="0.001" />
          <origin xyz="0 0 0" rpy="0 0 0"/>
          <inertia ixx="0.00000015" ixy="0" ixz="0" iyy="0.00000015" iyz="0" izz="0.00000015" />
      </inertial>
  </link>

  <joint name="imu_sensor_joint" type="fixed">
      <axis xyz="0 0 1" />
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <parent link="base_link"/>
      <child link="imu_sensor_link"/>
  </joint>
  
</robot>






